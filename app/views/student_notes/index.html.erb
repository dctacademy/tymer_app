<h1>Listing student_notes</h1>

<table>
  <thead>
    <tr>
      <th>Note</th>
      <th>User</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_notes.each do |student_note| %>
      <tr>
        <td><%= student_note.note %></td>
        <td><%= student_note.user_id %></td>
        <td><%= student_note.student_id %></td>
        <td><%= link_to 'Show', student_note %></td>
        <td><%= link_to 'Edit', edit_student_note_path(student_note) %></td>
        <td><%= link_to 'Destroy', student_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student note', new_student_note_path %>
