<div class="row">
  <div class="col-md-12">
    <div class="page-header">
      Total Tracker
    </div>
    
      <% data = {} %>
      <% Tracker.all.each do |tracker| %>
        <% data[:"#{tracker.start_datetime}"] =  ((tracker.end_datetime - tracker.start_datetime) / 60 / 60).to_f %> 
      <% end %>
      <%= line_chart(data) %>
    
  </div>
</div>